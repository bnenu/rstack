{
  "name": "rstack-starter",
  "version": "1.0.0",
  "description": "react stack starter kit",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --host 0.0.0.0 --port 3000 --inline --hot",
    "deploy": "NODE_ENV=production webpack -p --output-public-path /dist/",
    "test": "BABEL_DISABLE_CACHE=1 NODE_ENV=test tape -r babel-register src/**/*.spec.js",
    "serve": "NODE_ENV=production node server.js",
    "lint": "eslint --ext .js src/**"
  },
  "keywords": [
    "react",
    "es6",
    "hot-reloading",
    "css modules",
    "tape"
  ],
  "author": "Bogdan Nenu",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.11.0",
    "bootstrap": "^4.0.0-alpha.2",
    "express": "^4.13.4",
    "react": "^0.14.2",
    "react-dom": "^0.14.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.0.1",
    "redux": "^3.5.2",
    "redux-thunk": "^2.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel": "^6.0.15",
    "babel-core": "^6.1.2",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^6.0.1",
    "babel-plugin-webpack-loaders": "^0.4.0",
    "babel-preset-es2015": "^6.1.2",
    "babel-preset-react": "^6.1.2",
    "babel-preset-stage-0": "^6.1.18",
    "babel-register": "^6.8.0",
    "cheerio": "^0.19.0",
    "css-loader": "^0.23.0",
    "eslint": "^1.8.0",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-react": "^3.7.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "node-sass": "^3.7.0",
    "postcss": "^5.0.21",
    "postcss-loader": "^0.9.1",
    "react": "^15.0.2",
    "react-hot-loader": "^1.3.0",
    "redux": "^3.5.2",
    "redux-devtools": "^3.2.0",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.0",
    "tape": "^4.2.2",
    "webpack": "^1.12.3",
    "webpack-dev-server": "^1.14.0"
  }
